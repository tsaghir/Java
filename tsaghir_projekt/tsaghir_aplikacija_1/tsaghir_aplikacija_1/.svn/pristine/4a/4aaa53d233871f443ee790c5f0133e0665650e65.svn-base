        </navigation-case>
        <navigation-case>
            <from-outcome>dnevnik</from-outcome>
            <to-view-id>/pregledDnevnika.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>zahtjev</from-outcome>
